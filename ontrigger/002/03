public class Car {

    private int maxSpeed;
    private String color;
    private boolean isEngineOn;

    public Car(int maxSpeed, String color, boolean isEngineOn) {
        this.maxSpeed = maxSpeed;
        this.color = color;
        this.isEngineOn = isEngineOn;
    }

    public static void main(String[] args) {
        Car toyota = new Car(180, "Red", false);

        System.out.println("The car has a maximum speed of " + toyota.maxSpeed
                + ", is " + toyota.color
                + " and the boolean seems to be " + toyota.isEngineOn );
    }

}
