public class Task2  {
    public static void main(String[] args)  {
          int n1 = 100;
          int n2 = 20;
          
          System.out.println(n1 + n2);
          System.out.println(n1 - n2);
          System.out.println(n1 * n2);
          System.out.println(n1 / n2);
           System.out.println(n1 % n2);
    }
}
