import java.util.Scanner;

public class HelloWorld{

     public static void main(String []args){
         
         Scanner input = new Scanner(System.in);
         
         int health = 100;
         int experience = 0;
         int level = 1;
         
         String myLine = input.nextLine();
         
         if(myLine.equals("x"){
             experience+=5;
             health-=1;
             System.out.println("Attacking enemy caused you to have "+experience+" expirience points and "+health+" health.");
         }else if (myLine.equals("y"){
             experience-=1;
             health+=5;
             System.out.println("Healing yourself caused you to have "+health+" health and "+experience+" experience");
         }

     }
     
     //hopefully it works, I'm at work writing this
}
