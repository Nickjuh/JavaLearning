package com.company;

public class Conditions {
    public static void main(String[] args) {
        int nr1 = 21;
        int nr2 = 15;
        int sum = nr1 + nr2;
        if(sum >= 40){
                System.out.println ("It is bigger than 40.");
        }
        else if(sum <= 40 && sum >= 30){
            System.out.println ("It is between 30 and 40.");
        }


    }
}
