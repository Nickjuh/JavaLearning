// Program: xhl2_001_02.java
// Performs calculations of two integers, 100 and 20

public class xhl2_001_02 {
    public static void main(String[] args) {
        int a = 100;
        int b = 20;

        System.out.println(a + b);
        System.out.println(a - b);
        System.out.println(a * b);
        System.out.println(a / b);
        System.out.println(a % b);
    }
}



