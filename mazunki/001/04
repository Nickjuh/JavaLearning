class mazunki001_04 {

static int a = 25;
static int b = 70;


	public static int addIntegers(int x, int y){
		int z = x + y;

		System.out.println(x + " + " + y + " = " + z);


		return z;
	}


	public static void main(String[] args){

		addIntegers(a, b);


		return;
	}

}
