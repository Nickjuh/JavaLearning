class mazunki001_02 {

	public static void main(String[] args){
		
		int a = 100;
		int b = 20;

		System.out.println(a + "+" + b + " = " + (a+b));
		System.out.println(a + "-" + b + " = " + (a-b));
		System.out.println(a + "*" + b + " = " + (a*b));
		System.out.println(a + "/" + b + " = " + (a/b));
		System.out.println(a + "mod" + b + " = " + (a%b));


	}
}
